{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".wrangler/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "//#format:check": {},
    "//#format": {
      "cache": false
    },
    "//#lint:fix": {},
    "//#lint": {
      "cache": false
    },
    "//#check:fix": {},
    "//#check": {
      "cache": false
    }
  }
}
